{
  "name": "@digitalarkivet/mui-theme",
  "version": "3.1.4",
  "description": "MUI themes for Digitalarkivet applications",
  "private": false,
  "author": "Arkivvverket",
  "exports": {
    ".": "./dist/main.js",
    "./components/*": "./dist/components/*.js"
  },
  "license": "MIT",
  "homepage": "https://digitalarkivet.no",
  "keywords": [
    "mui",
    "digitalarkivet"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arkivverket/da-mui-theme.git"
  },
  "bugs": {
    "url": "https://github.com/arkivverket/da-mui-theme/issues"
  },
  "scripts": {
    "build": "tsc -p tsconfig.dist.json",
    "gallery": "pnpm webpack serve",
    "clean": "rm -r dist/*",
    "format": "prettier --write .",
    "format-ci": "prettier --list-different .",
    "ci": "pnpm install --frozen-lockfile",
    "self-up": "corepack use pnpm@latest",
    "outdated": "pnpm -r outdated",
    "up": "pnpm update --no-save # Update without changing package.json",
    "iup": "pnpm upgrade --interactive --latest",
    "rmnm": "rm -rf node_modules && rm -f pnpm-lock.yaml && rm -f package-lock.json",
    "depcheck": "pnpm dlx depcheck",
    "check-unmet-deps": "pnpm install --resolution-only"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@mui/x-date-pickers": "^8.22.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dayjs": "^1.11.19",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.5",
    "react-dom": "^19.2.3",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tailwind.css"
  ],
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
}