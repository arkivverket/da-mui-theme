{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2017",
      "es7",
      "es6",
      "dom"
    ],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "rootDir": "./src/",
    "jsx": "react-jsx",
    "sourceMap": true,
    "declarationMap": true,
    "paths": {
        "@digitalarkivet/mui-theme/*": ["./src/*"]
    },
  },
  "exclude": [
    "node_modules",
    "dist",
    "test",
    "gallery"
  ]
}
